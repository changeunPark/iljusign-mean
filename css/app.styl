/*	--------------------------------------------------
      <!-- Custom items Card  --!>
-------------------------------------------------- */

.card {
  box-shadow: 0 2px 5px 0 rgba(0, 0, 0, 0.16), 0 2px 10px 0 rgba(0, 0, 0, 0.12);
}

.card:hover {
  box-shadow: 0 14px 28px rgba(0,0,0,0.25), 0 10px 10px rgba(0,0,0,0.22);
}

.card {
  margin-top: 25px;
  box-sizing: border-box;
  background-color: #ffffff;
  background-clip: padding-box;
}

.card span.card-author {
    color: #fff;
    font-size: 1em;
    font-weight: 200;
    text-shadow: 1px 1px 2px #000000
}

.card .card-image {
  margin-top:25px;
  position: relative;
  overflow: hidden;
  transition: .5s ease;
}


.card .card-image img {
  border-radius: 5px 5px 0 0;
  background-clip: padding-box;
  position: relative;
  width:100%;
  height:200px;
}

.card .card-image span.card-author {
  position: absolute;
  bottom: 0;
  right: 0;
  padding: 10px;
}

.card .card-image span.card-type {
  position: absolute;
  bottom: 0;
  left: 0;
  padding: 10px;
}


.card .card-content {
  font-size: 1em;
  font-weight: 300;
  padding: 10px 20px 10px 20px;
  background-clip: padding-box;
  box-sizing: border-box;
}

.card .card-content p {
  margin: 0;
  color: inherit;
}

.card .card-content span.card-title {
  line-height: 30px;
}

.card .card-action {
  border-top: 1px solid rgba(160, 160, 160, 0.2);
  padding: 10px 20px 10px 20px;
}
